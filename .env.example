# Airtable Configuration
VITE_AIRTABLE_API_KEY=your_airtable_api_key
VITE_AIRTABLE_BASE_ID=your_airtable_base_id

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Google Gemini API (optional - for ICP generation)
VITE_GEMINI_API_KEY=your_gemini_api_key

# Anthropic API (for AI Chat - set as Supabase secret)
# Run: supabase secrets set ANTHROPIC_API_KEY=sk-ant-...
ANTHROPIC_API_KEY=your_anthropic_api_key

# Sentry Error Tracking (optional - for production)
VITE_SENTRY_DSN=your_sentry_dsn

# Test Configuration (optional - for E2E tests)
VITE_TEST_GC_EMAIL=test@example.com

